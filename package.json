{
  "name": "insider-case",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest --exclude=tests/e2e/**",
    "test:ui": "vitest --ui --exclude=tests/e2e/**",
    "test:run": "vitest run --exclude=tests/e2e/**",
    "test:watch": "vitest watch --exclude=tests/e2e/**",
    "test:coverage": "vitest run --coverage --exclude=tests/e2e/**",
    "test:ci": "vitest run --reporter=verbose --coverage --exclude=tests/e2e/**",
    "lint": "eslint . --ext  .vue,.js,.ts --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "lint:report": "eslint . --ext .vue,.js,.ts --format html --output-file eslint-report.html",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json}\"",
    "fix-all": "npm run format && npm run lint:fix",
    "type-check": "vue-tsc --noEmit",
    "code-quality": "npm run lint && npm run format:check && npm run type-check",
    "test:all": "npm run test:run && npm run fix-all && npm run type-check",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:all-with-e2e": "npm run test:run && npm run test:e2e && npm run code-quality"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "pinia": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": ">=9.27.0",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.4",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  }
}
