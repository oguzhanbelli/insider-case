{
  "name": "insider-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "lint:report": "eslint . --ext .vue,.js,.ts --format html --output-file eslint-report.html",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json}\"",
    "type-check": "vue-tsc --noEmit",
    "code-quality": "npm run lint && npm run format:check && npm run type-check"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": ">=9.27.0",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  }
}
